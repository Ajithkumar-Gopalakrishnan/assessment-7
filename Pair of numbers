n=int(input("enter the lenth of array:"))
arr=[]
for i in range(0,n):
	num=int(input("enter the elements"))
	arr.append(num)
print("array is" ,arr)
specificnumber=int(input("enter the number:"))
for i in range(0,n):
	for j in range(1,n):
		sum=arr[i]+arr[j]
		if(sum==specificnumber):
			print("the number is",arr[i],arr[j])
